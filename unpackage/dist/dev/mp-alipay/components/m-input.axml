<view class="m-input-view"><input class="m-input-input" focus="{{focus}}" type="{{inputType}}" placeholder="{{placeholder}}" password="{{type==='password'&&!showPassword}}" data-event-opts="{{[['input',[['onInput',['$event']]]],['focus',[['onFocus',['$event']]]],['blur',[['onBlur',['$event']]]]]}}" value="{{value}}" onInput="__e" onFocus="__e" onBlur="__e"/><block a:if="{{clearable&&!displayable&&value.length}}"><view class="m-input-icon"><m-icon vue-id="62e52ebd-1" color="#666666" type="clear" data-event-opts="{{[['^click',[['clear']]]]}}" onClick="__e" onVueInit="__l"></m-icon></view></block><block a:if="{{displayable}}"><view class="m-input-icon"><m-icon style="{{'color:'+(showPassword?'#666666':'#cccccc')+';'}}" vue-id="62e52ebd-2" type="eye" data-event-opts="{{[['^click',[['display']]]]}}" onClick="__e" onVueInit="__l"></m-icon></view></block></view>